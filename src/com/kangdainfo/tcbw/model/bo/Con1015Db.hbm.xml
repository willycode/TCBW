<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class 
    name="com.kangdainfo.tcbw.model.bo.Con1015Db" 
    table="CON1015_DB"
    lazy="false"
>
   <id
        name="id"
        type="java.lang.Long"
        column="ID"
    >
        <generator class="native">
            <param name="sequence">CON1015_DB_SEQ</param>
        </generator>
    </id>
    
    <many-to-one name="con1014Db" class="com.kangdainfo.tcbw.model.bo.Con1014Db" >
            <column name="CON1014_ID" not-null="true"  />
    </many-to-one>
    

    <many-to-one name="commonUser" class="com.kangdainfo.common.model.bo.CommonUser" >
            <column name="USER_ID" not-null="true"  />
    </many-to-one>

	
	<!-- 沒關聯的USERID 
	<property
        name="commonUser"
        type="java.lang.String"
        column="USER_ID"
        length="20"
    />
	-->
    <property
        name="competence"
        type="java.lang.String"
        column="COMPETENCE"
        length="20"
    />

    <property
        name="receiveSystem"
        type="java.lang.String"
        column="RECEIVESYSTEM"
        length="50"
    />
    <property
        name="receiveDate"
        type="java.lang.String"
        column="RECEIVEDATE"
        length="7"
    />
    <property
        name="receiveTime"
        type="java.lang.String"
        column="RECEIVETIME"
        length="6"
    />
    
    <property
        name="creator"
        type="java.lang.String"
        column="CREATOR"
        length="50"
    />
    <property
        name="createDate"
        type="java.lang.String"
        column="CREATEDATE"
        length="7"
    />
    <property
        name="createTime"
        type="java.lang.String"
        column="CREATETIME"
        length="6"
    />
    
    <property
        name="modifier"
        type="java.lang.String"
        column="MODIFIER"
        length="50"
    />
    <property
        name="modifyDate"
        type="java.lang.String"
        column="MODIFYDATE"
        length="7"
    />
    <property
        name="modifyTime"
        type="java.lang.String"
        column="MODIFYTIME"
        length="6"
    />
    
</class>
</hibernate-mapping>
