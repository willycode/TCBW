<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class 
    name="com.kangdainfo.tcbw.model.bo.Con0002Db" 
    table="CON0002_DB"
    lazy="false"
>
   <id
        name="id"
        type="java.lang.Long"
        column="ID"
    >
        <generator class="native">
            <param name="sequence">CON0002_DB_SEQ</param>
        </generator>
    </id>

    <property
        name="systemType"
        type="java.lang.String"
        column="SYSYTEMTYE"
        length="8"
    /> 
    
    <property
        name="formID"
        type="java.lang.String"
        column="FORMID"
        length="20"
    />
    <property
        name="title"
        type="java.lang.String"
        column="TITLE"
        length="100"
    />
    <property
        name="mailBody"
        type="java.lang.String"
        column="MAILBODY"
        length="4000"
    />
    
    <property
        name="applNo"
        type="java.lang.String"
        column="APPLNO"
        length="20"
    />
    
   <property
        name="state"
        type="java.lang.String"
        column="STATE"
        length="4"
    />
 
    <property
        name="receiveSystem"
        type="java.lang.String"
        column="RECEIVESYSTEM"
        length="50"
    />
    <property
        name="receiveDate"
        type="java.lang.String"
        column="RECEIVEDATE"
        length="7"
    />
    <property
        name="receiveTime"
        type="java.lang.String"
        column="RECEIVETIME"
        length="6"
    />
    
    <property
        name="creator"
        type="java.lang.String"
        column="CREATOR"
        length="50"
    />
    <property
        name="createDate"
        type="java.lang.String"
        column="CREATEDATE"
        length="7"
    />
    <property
        name="createTime"
        type="java.lang.String"
        column="CREATETIME"
        length="6"
    />
    
    <property
        name="modifier"
        type="java.lang.String"
        column="MODIFIER"
        length="50"
    />
    <property
        name="modifyDate"
        type="java.lang.String"
        column="MODIFYDATE"
        length="7"
    />
    <property
        name="modifyTime"
        type="java.lang.String"
        column="MODIFYTIME"
        length="6"
    />
    
</class>
</hibernate-mapping>
