<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class 
    name="com.kangdainfo.tcbw.model.bo.Drg1001Db" 
    table="DRG1001_DB"
    lazy="false"
>
   <id
        name="id"
        type="java.lang.Long"
        column="ID"
    >
        <generator class="native">
            <param name="sequence">DRG1001_DB_SEQ</param>
        </generator>
    </id>

    <property
        name="dpdKind"
        type="java.lang.String"
        column="DPDKIND"
        length="4"
    /> 
    
    <property
        name="dpdKindName"
        type="java.lang.String"
        column="DPDKINDNAME"
        length="50"
    />
    <property
        name="isCheckFile"
        type="java.lang.String"
        column="ISCHECKFILE"
        length="1"
    />    
    <property
        name="isStop"
        type="java.lang.String"
        column="ISSTOP"
        length="1"
    />
 
    <property
        name="receiveSystem"
        type="java.lang.String"
        column="RECEIVESYSTEM"
        length="50"
    />
    <property
        name="receiveDate"
        type="java.lang.String"
        column="RECEIVEDATE"
        length="7"
    />
    <property
        name="receiveTime"
        type="java.lang.String"
        column="RECEIVETIME"
        length="6"
    />
    
    <property
        name="creator"
        type="java.lang.String"
        column="CREATOR"
        length="50"
    />
    <property
        name="createDate"
        type="java.lang.String"
        column="CREATEDATE"
        length="7"
    />
    <property
        name="createTime"
        type="java.lang.String"
        column="CREATETIME"
        length="6"
    />
    
    <property
        name="modifier"
        type="java.lang.String"
        column="MODIFIER"
        length="50"
    />
    <property
        name="modifyDate"
        type="java.lang.String"
        column="MODIFYDATE"
        length="7"
    />
    <property
        name="modifyTime"
        type="java.lang.String"
        column="MODIFYTIME"
        length="6"
    />
    
</class>
</hibernate-mapping>
