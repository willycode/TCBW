<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class 
    name="com.kangdainfo.common.model.bo.EmailLog" 
    table="EmailLog"
    lazy="false"
    >
    <id
        name="id"
        type="java.lang.Long"
        column="ID"
    >
        <generator class="native">
            <param name="sequence">EMAILLOG_SEQ</param>
        </generator>
    </id>

    <property
        name="systemType"
        type="java.lang.String"
        column="systemType"
        length="8"
    /> 
    
    <property
        name="applNo"
        type="java.lang.String"
        column="applNo"
        length="20"
    />
   
    <property
        name="title"
        type="java.lang.String"
        column="title"
        length="100"
    />
    
    <property
        name="mail"
        type="java.lang.String"
        column="mail"
        length="200"
    />
     <property
        name="bccmail"
        type="java.lang.String"
        column="bccmail"
        length="200"
    />
     <property
        name="ccmail"
        type="java.lang.String"
        column="ccmail"
        length="200"
    />
    
    <property
        name="mailBody"
        type="java.lang.String"
        column="mailBody"
        length="4000"
    />
    
    <property
        name="state"
        type="java.lang.String"
        column="state"
        length="4"
    />

    <property
        name="failReasons"
        type="java.lang.String"
        column="failReasons"
        length="4000"
    />
    
    <property
        name="creator"
        type="java.lang.String"
        column="CREATOR"
        length="50"
    />
    <property
        name="createDate"
        type="java.lang.String"
        column="CREATEDATE"
        length="7"
    />
    <property
        name="createTime"
        type="java.lang.String"
        column="CREATETIME"
        length="6"
    />
    
    <property
        name="modifier"
        type="java.lang.String"
        column="MODIFIER"
        length="50"
    />
    <property
        name="modifyDate"
        type="java.lang.String"
        column="MODIFYDATE"
        length="7"
    />
    <property
        name="modifyTime"
        type="java.lang.String"
        column="MODIFYTIME"
        length="6"
    />
    
</class>
</hibernate-mapping>
